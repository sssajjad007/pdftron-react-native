  },
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://www.pdftron.com/documentation/mobile/#frameworks",
  "keywords": [
    "react-native",
    "PDFTron",
    "pdf",
    "office"
  ],
  "readmeFilename": "README.md",
  "dependencies": {
    "deprecated-react-native-prop-types": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "prop-types": "*"
  },
  "devDependencies": {
    "react": "*",
    "@types/react": "*",
    "@types/react-native": "*",
    "typescript": "*"
  }
