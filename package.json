    "postinstall": "py scripts/newPodspecMessage.py",
    "prestart": "py scripts/newPodspecMessage.py",
    "prepublishOnly": "bash scripts/prepare.sh",
    "postpublish": "bash scripts/postpublish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PDFTron/pdftron-react-native.git",
