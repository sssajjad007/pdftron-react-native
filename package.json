{
  "name": "react-native-pdftron",
  "title": "React Native Pdftron",
  "version": "3.0.3-20",
  "description": "React Native Pdftron",
  "main": "./lib/index.js",
  "typings": "index.ts",
  "scripts": {
    "copy-to-repo": "cd scripts && py fileCopy.py toRepo && cd ..",
    "copy-to-node-modules": "cd scripts && py fileCopy.py toModule && cd ..",
